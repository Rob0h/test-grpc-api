{
  "name": "test-grpc-api",
  "version": "0.0.1",
  "dependencies": {
    "@types/node": "^9.3.0",
    "async": "^1.5.2",
    "bluebird": "^3.5.1",
    "google-protobuf": "^3.0.0",
    "grpc": "^1.0.0",
    "lodash": "^4.6.1",
    "minimist": "^1.2.0",
    "nodemon": "^1.14.11",
    "ramda": "^0.25.0",
    "ts-protoc-gen": "^0.4.0",
    "typescript": "^2.6.2"
  },
  "scripts": {
    "start": "node build/greeter_server.js",
    "dev": "nodemon --watch ./src -e ts --exec \"npm run compile\"",
    "compile": "npm run build:ts && npm run build:proto && npm start",
    "build:ts": "tsc",
    "build:proto": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --js_out=import_style=commonjs,binary:generated --ts_out=service=true:generated -I ./protos ./protos/*.proto"
  }
}
